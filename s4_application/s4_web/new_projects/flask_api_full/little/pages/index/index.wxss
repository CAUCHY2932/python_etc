/* pages/index/index.wxss */

 .searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
} 


 /* 搜索框 */
.weui-search-bar {
position:relative;
padding:18rpx 37rpx 23rpx;
display:-webkit-box;
display:-webkit-flex;
display:flex;
box-sizing:border-box;
background-color:#ffffff;
border-top: none;
border-bottom:2rpx solid #f5f5f5;
}
.weui-search-bar__label {
position:absolute;
top:0;
right:0;
bottom:0;
left:0;
z-index:2;
border-radius:6rpx;
text-align:center;
color:#9b9b9b;
background:#fff;
/* height:60rpx; */
line-height:60rpx;
 align-items: center 
}
.weui-search-bar__box {
position:relative;
padding-left:60rpx;
padding-right:60rpx;
width:100%;
box-sizing:border-box;
z-index:1;
height: 60rpx;
}
.weui-search-bar__input {
height:60rpx;
line-height:60rpx;
font-size:28rpx;
}

.weui-search-bar__cancel-btn{
margin-left:20rpx;
line-height:60rpx;
color:#415d3d;
white-space:nowrap;
}

.weui-icon-search {
margin-right:16rpx;
font-size:inherit;
/* vertical-align: initial */
}

.weui-search-bar__text {
display:inline-block;
font-size:28rpx;
vertical-align: initial;
margin-top: -2rpx;
}


.weui-icon-search_in-box {
position:absolute;
left:20rpx;
top:18rpx;
}

.weui-icon-clear {
position:absolute;
top:0;
right:0;
padding:18rpx 16rpx;
font-size:0;

}


