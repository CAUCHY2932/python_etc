
.search{
  padding-bottom: 150rpx; 
}

.search-head{
  width: 100%;
  padding: 20rpx 37rpx 23rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #f9f9f9;
}

.search-head .search-words{
  border: 1rpx solid #f0f0f0;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 60rpx;
  flex-shrink: 0;
  border-radius: 4rpx;
}

.search-head .search-words .words{
  font-size: 28rpx;
  color: #6c6c6c;
}

.search-content{
  padding: 20rpx 37rpx;
}

.search-content .result{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  color: #9a9a9a;
  height: 66rpx;
  border-bottom: 1rpx solid #f9f9f9;
}

.maxText{
  font-size: 36rpx;
}

.none-data{
  margin-top: 120rpx;
}